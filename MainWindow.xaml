<Window x:Class="FakeNews.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakeNews"
        mc:Ignorable="d"
        Title="Fake News" SizeToContent="WidthAndHeight">
    <StackPanel Margin="50 10">
        <Button Name="ButtonOpen" Height="25" Width="100" Content="Open" Click="ButtonOpen_Click" />
        <Button Name="ButtonSave" Height="25" Margin="0 10 0 0" Content="Save" Click="ButtonSave_Click" />
    </StackPanel>
</Window>
